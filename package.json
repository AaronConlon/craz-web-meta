{
  "name": "craz-web-meta",
  "version": "1.0.0",
  "description": "A web metadata extraction service",
  "scripts": {
    "dev": "bun run --hot src/index.ts",
    "start": "bun run src/index.ts",
    "start:pm2": "pm2 start ecosystem.config.cjs --env production",
    "stop:pm2": "pm2 stop craz-web-meta",
    "restart:pm2": "pm2 restart craz-web-meta",
    "status:pm2": "pm2 status",
    "logs:pm2": "pm2 logs craz-web-meta",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:integration": "vitest run tests/",
    "test:invite": "vitest run tests/invite.test.ts",
    "test:unit": "vitest run tests/unit/",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "tsc --noEmit"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.7.0",
    "@prisma/client": "6.9.0",
    "cheerio": "1.0.0-rc.12",
    "hono": "^4.7.11",
    "ioredis": "^5.6.1",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/bun": "latest",
    "@types/cheerio": "^0.22.34",
    "@types/node": "^24.0.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitest/coverage-v8": "^3.2.3",
    "@vitest/ui": "^3.2.3",
    "prisma": "^6.9.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.3"
  },
  "type": "module",
  "license": "MIT",
  "packageManager": "bun@1.2.2"
}